    <div class="row" id="timeline-row">
      <div class="col-md-8">
        <div id="div-timeline"></div>
      </div>
      <div class="col-md-2">
        <h5>Filter</h5>
        <div id="div-legend"></div>
      </div>
    </div>
    <div class="row">
      <div class="col-md-9">
        <p style="text-align: justify;">
          {description_long}
        </p>
        <p style="text-align: justify;">
          {description_add}
        </p>
      </div>
    </div>
    <div class="row">
      <div class="col-md-10" role="main">
        {content}
      </div>
    </div>
  </div>
  <div class="empty-footer"></div>
  <link rel="stylesheet" type="text/css" href="lib/font-awesome/css/font-awesome.min.css">
