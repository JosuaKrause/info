<body id="top" onload="start()">
  <nav id="smt_header" class="navbar navbar-default sticky" role="navigation">
    <div class="container clearfix">
      <div class="navbar-header nav-line">
        <a id="namefield" class="navbar-brand" href="#top">{name}</a>
      </div>
      <div class="nav navbar-nav navbar-right nav-line">
        <a href="https://medium.com/@josua.krause"><img src="img/mediumlogo_64x64.png" style="width: 32px; height: 32px;"></a>
        <a href="https://scholar.google.com/citations?user=hFjNgPEAAAAJ"><img src="img/scholarlogo_64x64.png" style="width: 32px; height: 32px;"></a>
        <a href="https://www.researchgate.net/profile/Josua-Krause"><img src="img/researchgatelogo_64x64.png" style="width: 32px; height: 32px;"></a>
        <a href="https://www.linkedin.com/in/josuakrause/"><img src="img/linkedinlogo_72x64.png" style="width: 32px; height: 32px;"></a>
        <a href="https://github.com/JosuaKrause/"><img src="img/GitHub-Mark-32px.png" style="width: 32px; height: 32px;"></a>
        <img src="img/photo_137x128.png" style="width: 64px; height: 64px;">
      </div>
    </div>
  </nav>
  <footer>
    <div class="footer-style">
      Â© {copyright} <a href="https://github.com/JosuaKrause/info">[source]</a>
    </div>
