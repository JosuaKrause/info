  />
  <meta
    name="description"
    property="og:description"
    content="{description}"
  />
  <meta
    property="og:image"
    content="https://www.josuakrause.com/{ogimg}"
  />
  <meta property="og:image:type" content="image/png" />
  <meta property="og:image:width" content="675" />
  <meta property="og:image:height" content="630" />
  <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:domain" content="www.josuakrause.com" />
  <meta
    name="twitter:url"
    content="https://www.josuakrause.com/"
  />
  <meta
    name="twitter:title"
    content="{name}"
  />
  <meta
    name="twitter:description"
    content="{description}"
  />
  <meta
    name="twitter:image"
    content="https://www.josuakrause.com/{ogimg}"
  />
  <link rel="canonical" href="https://www.josuakrause.com/">
  <link rel="stylesheet" type="text/css" href="lib/jk-js/lib/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="js/timeline.css">
  <link rel="stylesheet" type="text/css" href="js/index.css">
  <script src="lib/jk-js/lib/d3/d3.min.js" charset="utf-8" async="true"></script>
  <script src="js/timeline.js" charset="utf-8" async="true"></script>
  <script type="text/javascript">
    {js}
  </script>
  <script type="application/ld+json">
    {knowledge}
  </script>
