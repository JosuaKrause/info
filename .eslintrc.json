{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "rules": {
    "require-jsdoc": "off",
    "no-implicit-globals": "error",
    "no-undef": "error"
  },
  "settings": {
    "html/html-extensions": [".html", ".tmpl"]
  },
  "plugins": ["eslint-plugin-html"],
  "ignorePatterns": ["lib/"]
}
